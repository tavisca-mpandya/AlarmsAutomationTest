{
	"description": "added from postman333",
	"friendlyname": "abc",
	"threshold": {
		"value": 10,
		"condition": "lte"
	},
	"queryConfiguration": {
		"query": "app_name:hotel_engine AND type:exception",
		"index": "logs-*",
		"windowInSeconds": 300,
		"aggregate": {
			"type": "Percentile",
			"field": "*",
			"percents": []
		}
	},
	"notifications": {
		"onError": [
			{
				"type": "teams_message",
				"template": "failed_calls_failure"
			}
		],
		"onRecovery": [
			{
				"type": "twilio_call",
				"template": "failed_calls_failure"
			}
		]
	}
}